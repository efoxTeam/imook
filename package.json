{
  "name": "@efox/imook-workspace",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/efoxTeam/imook",
  "license": "MIT",
  "private": true,
  "devDependencies": {
    "lerna": "^3.18.3",
    "commitlint": "^11.0.0",
    "husky": "^4.3.6",
    "lint-staged": "^10.5.3",
    "@efox/emp-tsconfig": "^1.1.1",
    "@efox/eslint-config-react-prittier-ts": "^1.2.1"
  },
  "workspaces": {
    "packages":["packages/**","examples/*"]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
